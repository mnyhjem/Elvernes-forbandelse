@model List<ElvenCurse.Core.Model.Character>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="row">
    <div class="col-md-4">
        @Html.ActionLink("New character", "Create", "Character", new {@class="btn btn-default"})
        <ul>
            @foreach (var character in Model)
            {
                <li>@character.Name</li>
            }
        </ul>
    </div>
    <div class="col-md-8">
        billede af figuren
    </div>
</div>